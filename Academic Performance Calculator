#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int a,b,c,d,e,f;
    float g,h;
    scanf("%d", &a);
    scanf("%d", &b);
    scanf("%d", &c);
    scanf("%d", &d);
    scanf("%d", &e);
    f = a+b+c+d+e;
    g = f/5;
    h = f/5;
    printf("%d %.2f %.2f", f,g,h);
    return 0;
}
