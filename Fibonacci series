#include <stdio.h>

int main(){
    int N, a=0,b=1,c;
    printf("Enter the number upto which you  want to display fibonacci series : ");
    scanf("%d", &N);
    if(N==1)
        printf("%d", a);
    else{
        printf("%d %d", a,b);
        for(int i=3; i<=N; i++){
            c = a+b;
            printf(", %d", c);
            a=b;
            b=c;
        }
    }
    return 0;
}
